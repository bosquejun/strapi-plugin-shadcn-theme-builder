    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "shadcn-theme-builder",
    "displayName": "Shadcn Theme Builder",
    "description": "Shadcn Theme Builder for Strapi is a plugin that brings a visual, real-time customization experience to your Strapi admin panel. Inspired by TweakCN, it allows you to easily generate, edit, and manage Shadcn UI themes without touching code."
