    "@strapi/icons": "^2.0.0-rc.30",
    "@strapi/plugin-graphql": "^5.31.2",
    "@strapi/utils": "^5.31.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "colord": "^2.9.3",
    "culori": "^4.0.2",
    "lucide-react": "^0.555.0",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.11.2",
    "react-intl": "^7.1.14",
    "react-syntax-highlighter": "^16.1.0",
